// given a integer and we find out range 1 to num how many no are are prime or how many not prime . print all those number.
//num=12;

let count=0;
let num=13;
for(let x=1; x<=num; x++){
for(let i=1;i<=x;i++){
if(x%i==0){
count++;
}
}
}
if(count==2){
console.log(x,"is prime");
}else{
console.log(x,"is not prime");
}